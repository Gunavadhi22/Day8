package Day8;
public class Employee {
		int Empid=23;
		String name="guna";
		static String officename="zoho";
		
		void main (int a, String name)
		{
			System.out.println(a+ " " + name + "  " + officename);
		}

		public static void main(String[] args) {
		 Employee obj=new Employee();
	       System.out.println(obj.Empid);
	       System.out.println(Employee.officename);
	       Employee.officename="tcs";
	       obj.main(23,"guna");
	       obj.main(54,"divya");
	       obj.main(62,"kaviya");
		}
	}

