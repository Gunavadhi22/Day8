package Day8;
class Dad2 {
    void property() {
        System.out.println("lands, money, house");
    }
}
class Daughter extends Dad2 {
    void show() {
        System.out.println("scooty");
    }
}
public class Dad {
    public static void main(String[] args) {
        Daughter obj = new Daughter();
        obj.property();  
        obj.show();      
    }
}
