package Day8;

public class Method {
    int age;
    String name;


    Method(int a, String str) {
        age = a;
        name = str;
    }

    void display() {
        System.out.println(age + "  " + name);
    }

    public static void main(String[] args) {
        Method obj = new Method(5, "guna");
        obj.display();
    }
}
